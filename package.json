    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vitest": "^0.5.4",
    "exponential-backoff": "^3.1.2",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "node-json-db": "^2.3.1",
    "playwright": "^1.50.1",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5",
